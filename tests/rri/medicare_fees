CREATE TABLE `medicare_phys_fee_schedule` (
  `year`       integer(4) NOT NULL DEFAULT '2019',
  `carrier` varchar(5)   NOT NULL DEFAULT ' ',
  `locality`       varchar(2) NOT NULL DEFAULT ' ',
  `hcpcs`  varchar(5) NOT NULL DEFAULT ' ',
  `modifier` varchar(2)   NOT NULL DEFAULT '  ',
  `non_fac_fee`   decimal(9,2) DEFAULT '0000000.00',
  `fac_fee`       decimal(9,2) DEFAULT '0000000.00',
  `filler`        varchar(1) DEFAULT ' ',
  `pctc_indicator`  varchar(1) DEFAULT ' ',
  `status_code` varchar(1) DEFAULT ' ',
  `mult_surg_indicator` varchar(1) DEFAULT ' ',
  `office_therapy_reduct_amt` decimal (9,2) DEFAULT '0000000.00',
  `fac_therapy_reduct_amt` decimal (9,2) DEFAULT '0000000.00',
  `opps_indicator` varchar(1) DEFAULT ' ',
  `opps_non_fac_fee_amt` decimal(9,2) DEFAULT '0000000.00',
  `opps_fac_fee_amt` decimal(9,2) DEFAULT '0000000.00',
  KEY (`year`),
  KEY (`carrier`),
  KEY (`locality`),
  KEY (`hcpcs`),
  KEY (`modifier`)
) ENGINE = InnoDB;